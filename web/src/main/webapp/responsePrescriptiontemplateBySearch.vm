
<form name='prescriptiontemplateForm' action='prescriptiontemplateAdd.action' class='niceform' method='POST'>
<table border='0' width='800' cellpadding='1' cellspacing='1' bgcolor='#999999'>
<theader>
<tr class='tableheader'>
<th>Action</th>

	<th>$i18n.get('node')</th>	
	
</tr>

<tr class='tableitem'>
<div id='inlineaddprescriptiontemplate' style='display:none;'>
<th><input type='button' id='saveprescriptiontemplate' value="  $i18n.get('save')  " onclick="javascript:validatePrescriptiontemplate('add')" onkeypress="nextfield='';"></th>
<th> <input type='text'  id='node' name='node' size='8' value='' onkeypress="formname='inlineprescriptiontemplateForm'; nextfield='saveprescriptiontemplate';"> </th>
</div>
</tr>

</theader>
<tbody>
#foreach($prescriptiontemplate in $prescriptiontemplates)
	#if($prescriptiontemplate.id)
	<tr class='tableitem' onDblClick="javascript:makeGetRequestAddPrescriptiontemplate($prescriptiontemplate.id, 'edit')">
	<td>
	<a href="javascript:makeGetRequestAddPrescriptiontemplate($prescriptiontemplate.id, 'edit')"><img src='images/edit.png' width='15' height='15' alt='Edit' /></a> 
	<a href='prescriptiontemplateDelete.action?id=$prescriptiontemplate.id'><img src='images/delete.png' width='15' height='15' alt='Delete' /></a>
	<a href='javascript:void(0)' onclick="window.open('prescriptiontemplateView.action?id=$prescriptiontemplate.id', 'View prescriptiontemplate', 'width=400, height=300')"> <img src='images/view.png' width='15' height='15' alt='View' /> </a>
	
	<input name='button' type='button' style='cursor:pointer;'  onclick="javascript:document.getElementById('menuprescriptiontemplate$prescriptiontemplate.id').style.display='block'; " ondblclick="javascript:document.getElementById('menuprescriptiontemplate$prescriptiontemplate.id').style.display='none'"  value='>>' >  
	
	<div id='menuprescriptiontemplate$prescriptiontemplate.id' style='border:thin #FF0000 solid;background-color:#b9c9fe;cursor:pointer; width:100; display: none; position:absolute' onclick="this.style.display='none'">
 		
	</div>
	</td>
	
		
		<td>$!prescriptiontemplate.node</td>
			
	</tr>
	#end
</tbody>
#end
</table>
</form>
