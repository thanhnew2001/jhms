			<td><label>$i18n.get('node'):</label></td> <td><input type='text' id='node' name='node' value='$!ethnic.node' size='15' value='' onkeypress="formname='ethnicForm'; nextfield='updateethnic';"> </td>
			

<tr><td colspan='6'>
<input id='updateethnic' type='button' value="  $i18n.get('save')  " onkeydown="javascript: validateEthnic('edit', document.getElementById('ethnicForm') )" onclick="javascript: validateEthnic('edit', document.getElementById('ethnicForm') )" onkeypress="formname='ethnicForm'; nextfield = '';">


&nbsp;

</td></tr>
</table>
</form>
</div>
</body>
