<?xml version='1.0'?>
<!DOCTYPE hibernate-mapping
        PUBLIC '-//Hibernate/Hibernate Mapping DTD 2.0//EN'
        'http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd'>
<hibernate-mapping>
        <class name='com.ph.hms.Prescriptiontemplatedetail' table='prescriptiontemplatedetail'>
		<id name='id' column='id' unsaved-value='0'>
				    <generator class='native'/>
		</id>
		
			 <many-to-one name='prescriptiontemplate' class='com.ph.hms.Prescriptiontemplate' column='prescriptiontemplate'/>		
		
			 <many-to-one name='drug' class='com.ph.hms.Drug' column='drug'/>		
		
			<property name='quantity'/>			
		
			<property name='timeuse'/>			
		
			<property name='quantityuse'/>			
		
			<property name='note'/>			
						          
        </class>
</hibernate-mapping>


