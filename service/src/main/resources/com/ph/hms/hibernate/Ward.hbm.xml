<?xml version='1.0'?>
<!DOCTYPE hibernate-mapping
        PUBLIC '-//Hibernate/Hibernate Mapping DTD 2.0//EN'
        'http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd'>
<hibernate-mapping>
        <class name='com.ph.hms.Ward' table='ward'>
		<id name='id' column='id' unsaved-value='0'>
				    <generator class='native'/>
		</id>
		
			<property name='node'/>			
		
			 <many-to-one name='district' class='com.ph.hms.District' column='district'/>
			 
			 <property name='lon'/>
		
			<property name='lat'/>		
						          
        </class>
</hibernate-mapping>


